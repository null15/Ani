package preRoundTimer

import ClosureTimers

init


    doAfter(5) ->
        preRoundTimer()
        nzInit()

function preRoundTimer()

    let t = CreateTrigger()
    for i = 1 to 3
        t.registerDialogButtonEvent(udg_Button[i])
    t.addAction() ->
        udg_Timer.start(5, null)
        udg_TB = udg_Timer.createTimerDialog()
        udg_TB.setTimeColor(colorA(0, 225, 255, 144))