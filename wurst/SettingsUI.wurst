package SettingsUI

public framehandle settingUI

init


public function settingUI(integer index)

    settingUI = BlzCreateFrame("SuperParent", BlzGetOriginFrame(ORIGIN_FRAME_GAME_UI, 0), 0, index)
    BlzFrameSetAbsPoint(settingUI, FRAMEPOINT_LEFT, -0.1, 0.5)

    BlzFrameSetVisible(settingUI, true)
